	<queryString>
		<![CDATA[SELECT P.nome as Nome,
    P.preco,
    P.quantidade as Quantidade,
    C.nome_empresa,
    C.nome_proprietario,
    C.email_proprietario,
    C.descricao,
    C.numero,
    C.obs,
    C.cliente,
    C.imagem,
    C.preco as Total
FROM
    tbvenda P
